<p id="notice"><%= notice %></p>

<h1>Listing Reportefacturas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reportefacturas.each do |reportefactura| %>
      <tr>
        <td><%= link_to 'Show', reportefactura %></td>
        <td><%= link_to 'Edit', edit_reportefactura_path(reportefactura) %></td>
        <td><%= link_to 'Destroy', reportefactura, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reportefactura', new_reportefactura_path %>
