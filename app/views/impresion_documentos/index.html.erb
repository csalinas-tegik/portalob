<p id="notice"><%= notice %></p>

<h1>Listing Impresion Documentos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @impresion_documentos.each do |impresion_documento| %>
      <tr>
        <td><%= link_to 'Show', impresion_documento %></td>
        <td><%= link_to 'Edit', edit_impresion_documento_path(impresion_documento) %></td>
        <td><%= link_to 'Destroy', impresion_documento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Impresion documento', new_impresion_documento_path %>
